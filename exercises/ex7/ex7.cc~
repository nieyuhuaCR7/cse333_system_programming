#include <iostream>
#include "Vector.h"

using namespace vector333;

int main() {
    Vector v1(1, 2, 3);
    Vector v2(4, 5, 6);

    Vector v3 = v1 + v2;
    Vector v4 = v1 - v2;
    float v5 = v1 * v2;

    Vector v6 = v1 * 2;
    Vector v7 = 2 * v1;

    std::cout << "v1: " << v1 << std::endl;
    std::cout << "v2: " << v2 << std::endl;
    std::cout << "v3: " << v3 << std::endl;
    std::cout << "v4: " << v4 << std::endl;
    std::cout << "v5: " << v5 << std::endl;
    std::cout << "v6: " << v6 << std::endl;
    std::cout << "v7: " << v7 << std::endl;

    return 0;
}
