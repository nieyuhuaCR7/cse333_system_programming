CXX = g++
CXXFLAGS = -Wall -std=c++11

ex6: ex6.o Vector.o
	$(CXX) $(CXXFLAGS) -o ex6 ex6.o Vector.o

ex6.o: ex6.cc Vector.h
	$(CXX) $(CXXFLAGS) -c ex6.cc

Vector.o: Vector.cc Vector.h
	$(CXX) $(CXXFLAGS) -c Vector.cc

clean:
	rm -f ex6 *.o *~
